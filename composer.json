{
    "name": "wyrihaximus/staticmap",
    "description": "PHP implementation of the Google Maps Static API",
    "license": "MIT",
    "keywords": [
        "maps",
        "static maps",
        "image processing",
        "graphics"
    ],
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com",
            "homepage": "http://wyrihaximus.net/"
        }
    ],
    "homepage": "http://wyrihaximus.net/projects/php/staticmap.html",
    "require": {
        "php": "^8.1",
        "imagine/imagine": "^1.3",
        "react/http": "^1.8"
    },
    "require-dev": {
        "ext-gd": "*",
        "wyrihaximus/test-utilities": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\StaticMap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\Tests\\StaticMap\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        },
        "platform": {
            "php": "8.1.13"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
